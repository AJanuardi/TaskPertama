@{
    ViewData["Title"] = "Our Valuable Product";
}
<h2>@ViewData["Title"]</h2>

<p>Many Product that You Can Buy</p>

<div class="text-center">
@{
        var x = ViewBag.items;
        foreach (var i in x)
        {
            <div>
                <h6>@i.nama</h6>
                <div>
                <img src=@i.foto width="200px">
                </div>
                <h6>Rating: @i.rating</h6>
                <h6>Harga: Rp.@i.harga</h6>
                <a href="/Product/Detail/@i.id">Detail</a>            
            </div>
            
        }
        
}
</div>
